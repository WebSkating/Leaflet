*{
	margin:0;
	padding:0;
}
html,body{
	height: 100%;
	overflow: hidden;
}
.container,.swiper-container {
	text-align: center;
    position: absolute;
    top: 0;
    left: 0;
    width: 100%;
    height: 100%; 
} 
.page1,.page2,.page3,.page4,.page5,.page6{
	position: relative;
}
.fot_arrow{
	width: 100%;
	height: 55px;
	position: absolute;
	left: 0;
	bottom: 0;
	display: flex;
	justify-content: center;
	align-items: top;
}
.arrow{
	position: absolute;
	top: 0;
	margin: 0 auto;
	width:50px;
	height:100%;	
	left: 50%;
	margin-left: -20px;

}
.arrow img{
	position: absolute;
	left: 0;
	bottom: 0;
	width: 100%;
	height: 35px;
	animation: arrow infinite 1.5s ease-in-out;
}

@keyframes arrow{
	0%,
	30%{opacity:0;-webkit-transform:translate(0,10px)}
	60%{opacity:1;-webkit-transform:translate(0,0)}
	100%{opacity:0;-webkit-transform:translate(0,0)}
	}
.bt_bg{
	position: absolute;
	left: 0;
	bottom: 0;
	width: 100%;
	height:100%;
}
img{
	width:100%;
	height:100%;
}
